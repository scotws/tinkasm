; Test file for 
; A Tinkerer's Assembler for the 6502/65c02/65816

    .mpu 65816
    .origin 0e000

    vga_base = 8000
    vga1 = vga_base + 1
    vga2 = vga_base + 2
    vga3 = vga_base + 3
    vga4 = vga_base + 4

loop

            lda vga4
            cmp.# $01
            bne loop 

    .end 
